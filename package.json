{
  "name": "terminal",
  "version": "1.0.0",
  "description": "Terminal for eduk8s.",
  "main": "built/backend/server.js",
  "scripts": {
    "compile": "rm -rf built && tsc && browserify built/frontend/scripts/eduk8s.js --standalone eduk8s -o built/frontend/scripts/eduk8s-bundle.js && cat built/frontend/scripts/eduk8s-bundle.js | uglifyjs > built/frontend/scripts/eduk8s-bundle.min.js",
    "start": "node built/backend/server.js",
    "install": "npm run compile"
  },
  "author": "Graham Dumpleton",
  "license": "Apache-2.0",
  "dependencies": {
    "css-element-queries": "^1.2.3",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.19",
    "node-pty": "^0.9.0",
    "path": "^0.12.7",
    "pug": "^3.0.0",
    "requirejs": "^2.3.6",
    "split-grid": "^1.0.9",
    "ws": "^7.3.1",
    "xterm": "^4.7.0",
    "xterm-addon-fit": "^0.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jquery": "^3.5.0",
    "@types/node": "^14.0.20",
    "@types/ws": "^7.2.6",
    "browserify": "^16.5.1",
    "typescript": "^3.9.6",
    "uglify-js": "^3.10.0"
  }
}
